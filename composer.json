{
    "name": "syahdan/codeigniter-datatables",
    "description": "Codeigniter DataTables is a package that handles the server side works of DataTables with Codeigniter 4.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "syahdans\\DataTables\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Sufilal Syahdan",
            "email": "sufilal.syahdan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "prefer-stable": true,
    "require-dev": {
        "codeigniter4/devkit": "^1.1"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
